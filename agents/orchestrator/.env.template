# Orchestrator Configuration

# Port to listen on
PORT=8000

# Agent discovery endpoints (comma-separated)
# For local testing
AGENT_ENDPOINTS=http://localhost:8080/.well-known/agent.json

# For Kubernetes
# AGENT_ENDPOINTS=http://travel-agent-service/.well-known/agent.json

# Azure Service Bus (optional, for async communication)
SERVICEBUS_NAMESPACE=multiagent-dev-servicebus.servicebus.windows.net

# Authentication
# Set to 'false' for local development (uses Azure CLI)
# Set to 'true' for AKS deployment (uses Workload Identity)
USE_MANAGED_IDENTITY=false
